<%= form_for @owner, url: login_path, method: :post do |f| %>
  <br>
  <%= f.label :email %>
  <%= f.email_field :email %>
  <br><br>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <br><br>
  <%= f.submit "log in" %>
  or <%= link_to(controller: 'owners', action: 'new') { 'Sign up here' } %>
<% end %>